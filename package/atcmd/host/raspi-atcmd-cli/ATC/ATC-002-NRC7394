
ECHO " ATC-002 : NRC7394 ADC"
ECHO "========================================="
HOLD

AT+GPIOCONF?
AT+GPIOVAL?

AT+GPIOCONF=10,1,1
AT+GPIOCONF?=10
AT+GPIOCONF=11,1,1
AT+GPIOCONF?=11

LOOP 10 3
AT+GPIOVAL=10,0
AT+GPIOVAL?=10
AT+GPIOVAL=11,1
AT+GPIOVAL?=10
WAIT 1s
AT+GPIOVAL=10,1
AT+GPIOVAL?=10
AT+GPIOVAL=11,0
AT+GPIOVAL?=11
WAIT 1s

AT+GPIOCONF=10,0,0
AT+GPIOCONF=11,0,0

AT+GPIOCONF?
AT+GPIOVAL?

