
ECHO " ATC-002 : NRC7292 GPIO"
ECHO "========================================="
HOLD

AT+GPIOCONF?
AT+GPIOVAL?

AT+GPIOCONF=8,1,1
AT+GPIOCONF?=8
AT+GPIOCONF=9,1,1
AT+GPIOCONF?=9

LOOP 10 3
AT+GPIOVAL=8,0
AT+GPIOVAL?=8
AT+GPIOVAL=9,1
AT+GPIOVAL?=8
WAIT 1s
AT+GPIOVAL=8,1
AT+GPIOVAL?=8
AT+GPIOVAL=9,0
AT+GPIOVAL?=9
WAIT 1s

AT+GPIOCONF=8,0,0
AT+GPIOCONF=9,0,0

AT+GPIOCONF?
AT+GPIOVAL?

